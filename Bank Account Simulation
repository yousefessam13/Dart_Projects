class BankAccount {
  double balance = 0;
  void deposit(double amount) {
    balance += amount;
    print("Deposited: $amount");
  }
  void withdraw(double amount) {
    if (amount <= balance) {
      balance -= amount;
      print("Withdrew: $amount");
    } else {
      print("Not enough balance");
    }
  }
  void checkBalance() {
    print("Balance: $balance");
  }
}
 main() {
  BankAccount a = BankAccount();
  a.deposit(500.15);
  a.withdraw(200);
  a.checkBalance();
}
